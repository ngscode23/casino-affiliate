  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx --report-unused-disable-directives",
    "preview": "vite preview",
    "clean": "rimraf node_modules package-lock.json .turbo .vite .cache dist",
    "lint:fix": "eslint . --fix",
    "prepare": "husky",
    "test": "vitest run --passWithNoTests --reporter verbose",
    "test:watch": "vitest --reporter verbose --watch",
    "test:coverage": "vitest run --coverage --reporter verbose",
    "test:coverage:watch": "vitest --coverage --reporter verbose --watch",
    "db:migrate": "drizzle-kit push --env production",
    "db:generate": "drizzle-kit generate --env production",
    "db:generate:dev": "drizzle-kit generate --env development",
    "db:generate:test": "drizzle-kit generate --env test",
    "db:reset": "drizzle-kit reset --env production",
    "db:reset:dev": "drizzle-kit reset --env development",
    "db:reset:test": "drizzle-kit reset --env test",
    "db:seed": "drizzle-kit seed --env production",
    "db:seed:dev": "drizzle-kit seed --env development"
  },
